{
    "name": "imcger/radialprogbar",
    "type": "phpbb-extension",
    "description": "This extension change the linear progress bar to a radial progress bar.",
    "homepage": "https://github.com/IMC-GER/phpBB-Radial-Progressbar/",
    "version": "1.0.3",
    "time": "2024-10-07",
    "keywords": ["phpbb", "extension", "radialprogbar"],
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Thorsten Ahlers",
            "homepage": "https://github.com/IMC-GER",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "phpbb/phpbb": "^3.3.0",
        "composer/installers": "~1.0"
    },
    "extra": {
        "display-name": "Radial Progressbar",
        "soft-require": {
            "phpbb/phpbb": "^3.3.0"
        },
        "version-check": {
            "host": "raw.githubusercontent.com",
            "directory": "/IMC-GER/phpBB-Radial-Progressbar/master",
            "filename": "radialprogbar_version.json",
            "ssl": true
        }
    }
}
